{
  "name": "quipucords-ui",
  "version": "2.0.0",
  "description": "Quipucords UI",
  "author": "Red Hat",
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/quipucords/quipucords-ui.git"
  },
  "bugs": {
    "url": "https://github.com/quipucords/quipucords-ui/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.25%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "prebuild": "npm run type-check && npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "export NODE_ENV=production; webpack --config ./config/webpack.prod.js && npm run dr:surge",
    "start": "export NODE_ENV=development; webpack serve --color --progress --config ./config/webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .ts,.tsx,.js ./src/",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint -- --fix",
    "format": "npm run lint:fix",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "webpack --config ./config/webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.3.0",
    "@tanstack/react-query-devtools": "^5.8.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "^29.5.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.3.3",
    "html-replace-webpack-plugin": "^2.6.0",
    "html-webpack-plugin": "^5.5.3",
    "imagemin": "^8.0.1",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "mini-css-extract-plugin": "^2.7.6",
    "mini-svg-data-uri": "^1.4.4",
    "moxios": "^0.4.0",
    "npm-check-updates": "^16.11.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.25",
    "prettier": "^3.0.0",
    "raw-loader": "^4.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^5.0.1",
    "sass": "^1.65.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@mturley-latest/react-table-batteries": "^1.0.4",
    "@patternfly/react-core": "^5.0.0",
    "@patternfly/react-icons": "^5.0.0",
    "@patternfly/react-styles": "^5.0.0",
    "@patternfly/react-table": "^5.1.1",
    "@tanstack/react-query": "^5.8.4",
    "axios": "^1.6.1",
    "detect-browser": "^5.3.0",
    "i18next": "^23.4.4",
    "i18next-xhr-backend": "^2.0.1",
    "moment": "^2.29.4",
    "react": "^18",
    "react-axe": "^3.5.4",
    "react-dom": "^18",
    "react-i18next": "^13.1.2",
    "react-redux": "^8.1.2",
    "react-router-dom": "6.4.0",
    "sirv-cli": "^2.0.2"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}
